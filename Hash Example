//hashing example 

#include <iostream>
#include <string>
using namespace std;

class myhash
{
  private:
    int tablesize=10;

  
  public:
          myhash();
          int Hash(string key);
   
};

myhash::myhash()
{
  tablesize=100;
}


int myhash::Hash(string key)
{
  
  int hash=0;
  int index=0;
  
  for( int i=0;i<key.length();i++)
  {
    hash +=int(key[i]);
    cout<<"Index "<<hash <<endl;
  }
  
  index=hash%tablesize;
  
  return index;
}
  


// To execute C++, please define "int main()"
int main() {
  
  myhash besthashna;
  cout<<"Hash Index "<<besthashna.Hash("yellow")<<endl;
    
  
  
  return 0;
}
